@article{merkerConsciousnessCerebralCortex2007,
  ids = {merkerConsciousnessCerebralCortex2007a,merkerConsciousnessCerebralCortex2007b,merkerConsciousnessCerebralCortex2007c},
  title = {Consciousness without a Cerebral Cortex: {{A}} Challenge for Neuroscience and Medicine},
  shorttitle = {Consciousness without a Cerebral Cortex},
  author = {Merker, Bjorn},
  year = {2007},
  month = feb,
  journal = {Behavioral and Brain Sciences},
  volume = {30},
  number = {1},
  pages = {63--81},
  publisher = {{Cambridge University Press}},
  issn = {1469-1825, 0140-525X},
  doi = {10.1017/S0140525X07000891},
  abstract = {A broad range of evidence regarding the functional organization of the vertebrate brain \textendash{} spanning from comparative neurology to experimental psychology and neurophysiology to clinical data \textendash{} is reviewed for its bearing on conceptions of the neural organization of consciousness. A novel principle relating target selection, action selection, and motivation to one another, as a means to optimize integration for action in real time, is introduced. With its help, the principal macrosystems of the vertebrate brain can be seen to form a centralized functional design in which an upper brain stem system organized for conscious function performs a penultimate step in action control. This upper brain stem system retained a key role throughout the evolutionary process by which an expanding forebrain \textendash{} culminating in the cerebral cortex of mammals \textendash{} came to serve as a medium for the elaboration of conscious contents. This highly conserved upper brainstem system, which extends from the roof of the midbrain to the basal diencephalon, integrates the massively parallel and distributed information capacity of the cerebral hemispheres into the limited-capacity, sequential mode of operation required for coherent behavior. It maintains special connective relations with cortical territories implicated in attentional and conscious functions, but is not rendered nonfunctional in the absence of cortical input. This helps explain the purposive, goal-directed behavior exhibited by mammals after experimental decortication, as well as the evidence that children born without a cortex are conscious. Taken together these circumstances suggest that brainstem mechanisms are integral to the constitution of the conscious state, and that an adequate account of neural mechanisms of conscious function cannot be confined to the thalamocortical complex alone.},
  langid = {english},
  pmid = {17475053},
  file = {c://Users/Jonathan/Google Drive/.bibliography/zotero_pdf/Merker_2007_Consciousness without a cerebral cortex.pdf;c://Users/Jonathan/Google Drive/.bibliography/zotero-pdf/Merker_2007_Consciousness without a cerebral cortex2.pdf;c://Users/Jonathan/Google Drive/.bibliography/zotero-pdf/Merker_2007_Consciousness without a cerebral cortex3.pdf;c://Users/Jonathan/zotero/storage/6WNZ99JF/C9B1B393176EF250D4AFBB3054A04E31.html;c://Users/Jonathan/zotero/storage/SQQ9J73C/C9B1B393176EF250D4AFBB3054A04E31.html}
}

