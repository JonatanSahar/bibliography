  issn = {0956-7976, 1467-9280},
  doi = {10.1177/0956797617735745},
  abstract = {Is consciousness necessary for integration? Findings of seemingly high-level object-scene integration in the absence of awareness have challenged major theories in the field and attracted considerable scientific interest. Lately, one of these findings has been questioned because of a failure to replicate, yet the other finding was still uncontested. Here, we show that this latter finding\textemdash slowed-down performance on a visible target following a masked prime scene that includes an incongruent object\textemdash is also not reproducible. Using Bayesian statistics, we found evidence against unconscious integration of objects and scenes. Put differently, at the moment, there is no compelling evidence for object-scene congruency processing in the absence of awareness. Intriguingly, however, our results do suggest that consciously experienced yet briefly presented incongruent scenes take longer to process, even when subjects do not explicitly detect their incongruency.},
  langid = {english},
  file = {c://Users/Jonathan/Google Drive/.notes/.bibliography/zotero_pdf/Biderman_Mudrik_2018_Evidence for Implicit—But Not Unconscious—Processing of Object-Scene Relations.pdf}
}

